{
  "propertyEditors": [
    {
      "alias": "Camelonta.ImageCropper",
      "name": "Camelonta.ImageCropper",
      "icon": "icon-map-location",
      "editor": {
        "view": "~/App_Plugins/Camelonta.ImageCropper/ImageCropper.html"
      },
      "prevalues": {
        "fields": [
          {
            "label": "Width",
            "description": "Enter the pixel width of the crop",
            "key": "width",
